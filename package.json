{
  "name": "@microlink/cli",
  "description": "Interacting with Microlink API from your terminal.",
  "homepage": "https://nicedoc.io/microlinkhq/cli",
  "version": "1.5.8",
  "bin": {
    "microlink": "bin/microlink"
  },
  "author": {
    "email": "hello@microlink.io",
    "name": "microlink.io",
    "url": "https://microlink.io"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/cli.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/cli/issues"
  },
  "keywords": [
    "api",
    "cli",
    "microlink"
  ],
  "dependencies": {
    "@microlink/mql": "~0.9.3",
    "chalk": "~4.1.0",
    "clipboardy": "~2.3.0",
    "escape-string-regexp": "~4.0.0",
    "got": "~11.8.2",
    "jsome": "~2.5.0",
    "meow": "~9.0.0",
    "ora": "~5.4.0",
    "pretty-bytes": "~5.6.0",
    "pretty-ms": "~7.0.1",
    "temperment": "~1.0.0",
    "term-img": "~5.0.0",
    "terminal-link": "~2.1.1",
    "update-notifier": "~5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "exit 0",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "preferGlobal": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "package.json": [
      "finepack"
    ],
    "*.js,!*.min.js,": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx lint-staged"
  }
}
